{
    "name": "inextenso/cua",
    "type": "library",
    "description": "This application can manage many project composer update",
    "keywords": ["composer", "update", "dependecies", "management"],
    "homepage": "https://github.com/InExtenso/cua",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Baptiste Nahan",
            "email": "jean-baptiste.nahan@inextenso.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "InExtenso\\CUA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InExtenso\\CUA\\Tests\\Units\\": "tests/Units"
        }
    },
    "bin": ["cua"],
    "require": {
        "php": ">=5.6",
        "symfony/console": "^3.0",
        "symfony/yaml": "^3.0",
        "symfony/config": "^3.0",
        "psr/log": "^1.0",
        "symfony/process": "^3.0",
        "symfony/var-dumper": "^3.0",
        "symfony/finder": "^3.0",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "atoum/atoum": "^3.0",
        "atoum/autoloop-extension": "*",
        "atoum/reports-extension": "*"
    }
}
